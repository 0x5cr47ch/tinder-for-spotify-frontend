{
  "expo": {
    "name": "Sonder",
    "slug": "sonder",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#121212",
        "image": "./assets/splash.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#121212"
      },
      "splash": {
        "backgroundColor": "#121212",
        "image": "./assets/splash.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sonder.music"
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "Sonder accesses your photos to let you share them with friends",
          "cameraPermission": "Sonder accesses your camera to let you take pictures and share them with friends"
        }
      ]
    ],
    "scheme": "sonder",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "679e31a3-80cc-4d3e-ae9c-a9e1f1a7b798"
      }
    },
    "owner": "fortune710",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/679e31a3-80cc-4d3e-ae9c-a9e1f1a7b798"
    }
  }
}
